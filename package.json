{
  "name": "@juancarllos-ui/react-native",
  "version": "1.1.8",
  "description": "Componentes mobile",
  "publishConfig": {
    "access": "public"
  },
  "files": ["src/react-native"],
  "scripts": {
    "style": "prettier . --check 'src/**/*.{ts,tsx,jsx,mdx}'",
    "style:fix": "prettier --write 'src/**/*.{ts,tsx,jsx,mdx}'",
    "lint:fix": "eslint src --fix",
    "lint": "eslint 'src/**/*.{ts,tsx,jsx,mdx}'",
    "prepare": "./script/prepare.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.19.0",
    "@storybook/addon-docs": "^8.5.2",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/addon-interactions": "^8.5.2",
    "@storybook/addon-onboarding": "^8.5.2",
    "@storybook/blocks": "^8.5.2",
    "@storybook/manager-api": "^8.5.3",
    "@storybook/react": "^8.5.2",
    "@storybook/react-native-web-vite": "^8.5.2",
    "@storybook/storybook-deployer": "^2.8.12",
    "@storybook/test": "^8.5.2",
    "@storybook/theming": "^8.5.3",
    "@testing-library/react-native": "^13.0.1",
    "@types/jest": "^29.5.14",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19.0.8",
    "babel-jest": "^29.7.0",
    "commitlint": "^15.0.0",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "react-native-web": "^0.19.13",
    "react-test-renderer": "^19.0.0",
    "storybook": "^8.5.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11"
  },
  "dependencies": {
    "@storybook/addon-actions": "^8.5.2",
    "@storybook/addon-links": "^8.5.2",
    "@storybook/react-native": "^8.5.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "phosphor-react-native": "^2.3.1",
    "react": "^19.0.0",
    "react-native": "^0.77.0",
    "react-native-svg": "^15.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,jsx,mdx}'": [
      "yarn style",
      "yarn lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
