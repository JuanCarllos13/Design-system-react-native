{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es5",
    "lib": ["dom", "es6"],
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
			"@assets/*": ["./src/assets/*"],
			"@components/*": ["./src/components/*"],
			"@pages/*": ["./src/pages/*"],
			"@storage/*": ["./src/storage/*"],
			"@styles/*": ["src/react-native/styles/*"],
			"@utils/*": ["./src/utils/*"],
			"@service/*": ["./src/service/*"],
			"@contexts/*": ["./src/contexts/*"]
		}
  },
  "include": ["src/**/*"]
}
